{{extends "templates/application.jet"}}

{{block body()}}
<div class="page content-page">
  <div class="container">
    <div class="page-header">
      <h1>{{ page.Title }}</h1>
    </div>

    {{if len(page.Content)}}
    <div class="content-page-content">
      {{page.Content | raw}}
    </div>
    {{end}}

  </div>

</div>
{{end}}